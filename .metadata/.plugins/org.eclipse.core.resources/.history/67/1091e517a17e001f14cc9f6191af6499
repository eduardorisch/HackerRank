package exs;

import java.util.Random;

public class exMatriz5 {

	public static void main(String[] args) {
		Random r = new Random();
		int [][]matriz = new int [4][4];
		int impares, pares;
		
		for (int i = 0; i < matriz.length; i++) {
			for (int j = 0; j < matriz.length; j++) {
				matriz[i][j] = r.nextInt();
}
		}
		
	}

}
